{
  "name": "kamiq",
  "version": "0.1.3",
  "description": "A lightweight node web framework for building delcarative web servers with high Express.js interoperability",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "private": "false",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "package": "npm pack && node tgzprocessing.js",
    "clean": "rm -rf dist",
    "produce": "npm run clean && npm i && npm run build && npm run package",
    "lint": "tsc",
    "release": "pnpm run build && changeset publish"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "boxen": "5.1.2",
    "chalk": "4.1.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-winston": "^4.2.0",
    "multer": "^1.4.5-lts.1",
    "readline": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "tar": "^6.1.15",
    "tsyringe": "^4.8.0",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "eslint": "^8.45.0",
    "nodemon": "^3.0.1",
    "prettier": "3.0.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  }
}
